<MudPaper Elevation="20" Width="100%" Style="margin: 16px 0; padding: 16px;">
    <MudText Class="d-flex align-center justify-content-center mud-width-full" Typo="Typo.h6">
        @Label
    </MudText>
    @foreach (var text in Content)
    {
        <MudText Style="height: 25px">@text</MudText>
    }
</MudPaper>

@code {
    
    [Parameter] public string Label { get; set; }
    [Parameter] public List<string> Content { get; set; }
}